<div class="search_bar">
  <div class="index_bar">
    <p>投稿一覧</p>
  </div>
  <div class="search_form">
    <%= form_with(url: search_rooms_path, local: true, method: :get, class: "form-input") do |form| %>
    <%= form.text_field :keyword, placeholder: "Please enter a keyword", class: "form-keyword" %>
    <%= form.submit "検索", class: "form-submit" %>
    <% end %>
  </div>
  
</div>